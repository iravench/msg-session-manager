{
  "name": "msg-session-manager",
  "version": "0.0.1",
  "description": "a messaging module, to host websocket connections",
  "main": "lib/",
  "scripts": {
    "start": "DEBUG=true nodemon --exec babel-node -- src/index.js | bunyan",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_init.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "compile": "rm -rf lib/ && babel -d lib/ src/ && cp src/plugins/*.lua lib/plugins/"
  },
  "author": "Raven Chen",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "amqplib": "^0.4.1",
    "body-parser": "^1.14.2",
    "bunyan": "^1.5.1",
    "engine.io": "^1.6.8",
    "engine.io-client": "^1.6.8",
    "express": "^4.13.3",
    "ioredis": "^1.15.1",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^3.10.1",
    "mirage": "0.0.5",
    "mysql": "^2.10.2",
    "primus": "^5.2.0",
    "primus-emitter": "^3.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  }
}
